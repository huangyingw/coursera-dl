[remote "coursera-dl"]
    url = https://github.com/coursera-dl/coursera-dl.git
    fetch = +refs/heads/*:refs/remotes/coursera-dl/*
[remote "vstrizhenok"]
    url = https://github.com/vstrizhenok/coursera-dl.git
    fetch = +refs/heads/*:refs/remotes/vstrizhenok/*
[remote "github"]
    url = git@github.com:huangyingw/coursera-dl.git
    fetch = +refs/heads/*:refs/remotes/github/*
[pull]
    from = vstrizhenok/master
[push]
    remote = github
[gsync]
    remote = coursera-dl
    branch = master
[merge]
	allow = true
[branch "dev"]
    remote = github
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[commit]
	message = n
[vrun]
	silent = false
	async = false
[deploy]
	host = localhost
	path = ~/myproject/git/AI/coursera/coursera-dl/coursera-dl
[gdif]
    ignore = .gitattributes
    ignore = .gitignore
    ignore = README.md
    ignore = post_handle.sh
    ignore = readme
    ignore = run.sh
